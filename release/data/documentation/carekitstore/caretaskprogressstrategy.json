      "content" : [
        {
          "anchor" : "overview",
          "level" : 2,
          "text" : "Overview",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "This structure is most commonly used when you already have access to an event and want to",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "specify a strategy for computing progress:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "let steps: OCKAnyEvent = getStepsEvent()",
            "let stepsProgress = steps.computeProgress(by: .summingOutcomeValues)"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "To create your own progress computation strategy, start by defining a data structure that stores task",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "progress. The data structure should conform to ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/edu.uky.cs.netreconlab.ParseCareKit\/documentation\/CareKitStore\/CareTaskProgress",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : ".",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "struct PhysicalTherapyProgress: CareTaskProgress {",
            "",
            "    \/\/ Custom properties here...",
            "",
            "    \/\/ Required by `CareTaskProgress`",
            "    var fractionCompleted: Double",
            "}"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "Next, extend ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/edu.uky.cs.netreconlab.ParseCareKit\/documentation\/CareKitStore\/CareTaskProgressStrategy",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " and write out logic for computing the custom progress",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "for a task.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "extension CareTaskProgressStrategy {",
            "",
            "    public static var customStrategy: CareTaskProgressStrategy<PhysicalTherapyProgress> {",
            "",
            "        return CareTaskProgressStrategy<PhysicalTherapyProgress> { event in",
            "            return computeCustomProgress(for: event)",
            "        }",
            "",
            "    }",
            "}"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "Now you can acess the strategy just like the other strategies in the framework.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "",
            "\/\/ Custom strategy.",
            "let physicalTherapy: OCKAnyEvent = getPhysicalTherapyEvent()",
            "let physicalTherapyProgress = physicalTherapy.computeProgress(by: .customStrategy)",
            "",
            "\/\/ Default strategy.",
            "let steps: OCKAnyEvent = getStepsEvent()",
            "let stepsProgress = steps.computeProgress(by: .summingOutcomeValues)",
            "",
            "\/\/ You can aggregate both default and custom progress data structures.",
            "let progress = AggregatedCareTaskProgress(combining: [stepsProgress, physicalTherapyProgress]"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        }
      ],
      "kind" : "content"
    }
